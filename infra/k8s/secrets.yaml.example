---
# GitLab Bulk Manager Secrets - EXAMPLE FILE
# Copy this file to secrets.yaml and replace placeholder values
#
# Generate secrets:
#   SESSION_SECRET: openssl rand -base64 64 | tr -d '\n'
#   REDIS_PASSWORD: openssl rand -base64 32 | tr -d '\n'
#
apiVersion: v1
kind: Secret
metadata:
  name: gitlab-bulk-manager-secrets
  namespace: gitlab-bulk-manager
  labels:
    app.kubernetes.io/name: gitlab-bulk-manager
    app.kubernetes.io/component: secrets
type: Opaque
stringData:
  # IMPORTANT: Replace these with generated values!
  SESSION_SECRET: "REPLACE_WITH_GENERATED_SECRET"
  REDIS_PASSWORD: "REPLACE_WITH_GENERATED_PASSWORD"
