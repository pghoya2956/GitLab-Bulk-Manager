# GitLab Configuration
VITE_GITLAB_URL=https://gitlab.com
VITE_GITLAB_TOKEN=your-gitlab-personal-access-token

# API Configuration
VITE_API_BASE_URL=http://localhost:3001/api
VITE_WEBSOCKET_URL=ws://localhost:3001

# Feature Flags
VITE_ENABLE_MOCK_DATA=false
VITE_ENABLE_WEBSOCKET=true
VITE_ENABLE_ANALYTICS=false

# Authentication
VITE_AUTH_TIMEOUT=3600000 # 1 hour in milliseconds
VITE_REFRESH_TOKEN_INTERVAL=1800000 # 30 minutes

# Application Settings
VITE_DEFAULT_LANGUAGE=ko
VITE_DEFAULT_THEME=light
VITE_MAX_UPLOAD_SIZE=10485760 # 10MB in bytes
VITE_PAGINATION_SIZE=25